<div class="card">
    <div id="titleDiv" class="card-body" style="color:black; background-color:cadetblue; font-family:fantasy; font-size:xx-large; text-align: center">
        REPORTE ESTADO DE CUPONES

    </div>
</div>

<div class="card-body">
    <div class="row">
        <div class="col-md-4 card-body">
            <label>Cliente: </label>
            <select class="select2 form-control custom-select" id="cboCliente" style="width: 100%; height:36px;"> </select>
        </div>
        <div class="col-md-4 card-body">
            <label>Zona: </label>
            <select class="select2 form-control custom-select" id="cboZona" style="width: 100%; height:36px;"> </select>
        </div>

        <div class="col-md-4 card-body">
            <label>Fecha desde: </label>
            <input id="dateFrom" type="date" name="Fecha desde" />
        </div>
        <div class="col-md-4 card-body">
            <label>Fecha hasta: </label>
            <input id="dateUp" type="date" name="Fecha hasta" />
        </div>
    </div>
    <br />
    <div class="row">
        <div class="card-body">
            <button type="button" class="btn btn-primary" onclick="SearchReportprincipal();"><i class="fas fa-search"></i> Buscar</button>
        </div>
    </div>
</div>
<br />
<div>
    <div class="table-responsive">
        <table id="tblReportCuotaStatus" class="table table-bordered table-striped" style="color: black; text-align:left; ">
            <thead class="thead-light">
                <tr>
                    <th scope="col">N° Prestamo</th>
                    <th scope="col">Zona</th>
                    <th scope="col">Cliente</th>
                    <th scope="col">DNI</th>
                    <th scope="col">Domicilio</th>
                    <th scope="col">Fecha inicio</th>
                    <th scope="col">Fecha fin</th>
                    <th scope="col">Total prestamo</th>
                    <th scope="col">Total pagado</th>
                    <th scope="col">Saldo</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

